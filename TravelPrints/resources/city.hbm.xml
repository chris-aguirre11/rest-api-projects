<?xml version='1.0' encoding='UTF-8'?>  
<!DOCTYPE hibernate-mapping PUBLIC  
 "-//Hibernate/Hibernate Mapping DTD 3.0//EN"  
 "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">  
  
 <hibernate-mapping>  
	 <class name="travelprints.persistence.objects.City" table="City">

		 <composite-id>
			<key-property name="cityName" column="city_name"  length="45"/>
			<key-property name="stateName" column="state_name"  length="45"/>
		 </composite-id>
		 
		 <!-- <property name="cityName" column="city_name"></property>  
		 <property name="stateName" column="state_name"></property>   -->
		 <property name="countryName" column="country_name"></property> 
		 <property name="latitude" column="latitude"></property> 
		 <property name="longitude" column="longitude"></property> 
	 </class>  
         
 </hibernate-mapping> 